<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rain.sparrow.system.menu.dao.TSMenuDao">

    <select id="searchTSMenuList" resultType="com.rain.sparrow.system.menu.entity.TSMenu">
        SELECT * FROM t_s_menu
        <where>
            <if test="parentMenuId!=null and parentMenuId!=''">
                parent_menu_id = #{parentMenuId}
            </if>
        </where>
    </select>

</mapper>